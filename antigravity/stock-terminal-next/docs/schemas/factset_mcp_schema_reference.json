    },
    "estimate_type_values": ["consensus_fixed", "consensus_rolling", "surprise", "ratings", "segments", "guidance"],
    "relationshipType_values": ["COMPETITORS", "CUSTOMERS", "SUPPLIERS", "PARTNERS"],
