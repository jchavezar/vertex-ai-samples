      "FactSet_People": ["profiles", "jobs", "company_people", "company_positions", "company_compensation", "company_stats"],
      "FactSet_Ownership": ["fund_holdings", "security_holders", "insider_transactions", "institutional_transactions"],
      "FactSet_Fundamentals": ["fundamentals"],
      "FactSet_MergersAcquisitions": ["deals_by_company", "public_targets", "deal_details"],
